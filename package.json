    "start-electron-renderer-dev": "webpack-dev-server --config webpack.electron.renderer.dev.config.js",
    "start-electron-main-dev": "webpack --config webpack.electron.main.config.js && electron ./dist/main.js",
    "start-electron-dev": "cross-env START_HOT=1 npm run start-electron-renderer-dev"
